def   generator_range(start, end, step):
    while start < end:
        yield start
        start += step

class iterator_range:
    def __init__(self, start, end, step):
        self.start = start - step
        self.end = end
        self.step = step
    def __iter__(self):
        return self
    def __next__(self):
        self.start += self.step
        if self.start < self.end:
            return self.start
        else:
            raise StopIteration
